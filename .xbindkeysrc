# Next track in mpc
"mpc next"
	XF86AudioNext

# Previous track in mpc
"mpc prev"
	XF86AudioPrev

# Play/Pause mpc
"mpc toggle"
	m:0x0 + c:172
	XF86AudioPlay

# Volume control
"amixer set Master 2%+"
	XF86AudioRaiseVolume

"amixer set Master 2%-"
	XF86AudioLowerVolume

"amixer set Master toggle"
	XF86AudioMute
